
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="@Url.Content("~/css/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/login.css")">

    <title>WEB日報管理システム - 山本建設株式会社</title>
</head>

<body>
    <canvas class='connecting-dots' id="experiment"></canvas>
    <div class="container">
        <div class="container">
            <div class="wrapper">
                <form action="" method="post" name="Login_Form" class="form-signin">
                    <h3 class="form-signin-heading">WEB日報管理システム</h3>

                    <h4 class="form-signin-heading">ログイン</h4>
                    <hr class="colorgraph"><br>

                    <input type="password" class="form-control" name="Password" placeholder="社員番号" required="" />

                    <button class="btn btn-lg btn-outline-secondary btn-block" name="Submit" value="Login" type="Submit">
                        ログイン
                    </button>
                    <h5 class="text-right pt-5">山本建設株式会社</h5>
                </form>
            </div>
        </div>
    </div>

    @*<script src="@Url.Content("~/js/calendar.js")"></script>*@
    <script src="@Url.Content("~/js/jquery-3.3.1.slim.min.js")"></script>
    <script src="@Url.Content("~/js/popper.min.js")"></script>
    <script src="@Url.Content("~/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/js/js.js")"></script>
</body>
</html>

